        }


QTabWidget {
    background-color: #ff7500;
}

    QTabWidget::pane {
        border-top: 3px solid #0094ff;
    }

QTabBar::tab {
    background: #efd4a9;
	border: 1px solid #dcc399;
    min-width: 8ex;
    padding: 5px 10px;
}

	QTabBar::tab:hover {
        background: #6fc2ff;
		border: 0px;
    }

    QTabBar::tab:selected {
        background: #0094ff;
		color: white;
		border: 0px solid #dcc399;
    }

    QTabBar::tab:!selected {
        margin-top: 5px;
    }

QTabBar::close-button {
	image: url(:/icons/close-tab.png);
	subcontrol-position: right;
}

QTabBar::close-button:!selected {
	margin-top: 5px;
 }
 QTabBar::close-button:hover {
image: url(:/icons/close-tab-hover.png);
 }
