   <string notr="true">/* Application-wide properties*/
* {
    color: black;
    font-size: 12pt;
}

QMainWindow {
    background-color: #e0ded8;
}

QMenuBar {
	background-color: #e0ded8;
	margin: 0px;
	border: 0px;
}

    QMenuBar::item {
        spacing: 3px; /* spacing between menu bar items */
        padding: 7px 12px;
        background: transparent;
    }

        QMenuBar::item:selected { /* when selected using mouse or keyboard */
			background: #efd4a9;
			border: 1px solid #dcc399;
        }

        QMenuBar::item:pressed {
			background: #efd4a9;
			border: 0px solid #dcc399;
        }

QMenu {
	margin: 0px;
	border: 0px;
}

    QMenu::item {
        padding: 12px 24px;
		background: #efd4a9;
    }

        QMenu::item:selected {
            background: #0094ff;
			color: white;
        }

    QMenu::separator {
        height: 1px;
		background-color: black;
    }

    QMenu::indicator {
        width: 13px;
        height: 13px;
    }


/* Actual controls*/

QLabel {
}

QPushButton {
background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #fff, stop:1 #ddd);
border: 1px solid #444;
    padding: 10px 20px;
}

    QPushButton:hover {
background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #fff, stop:1 #ddd);
border: 1px solid #444;
    }

    QPushButton:pressed {
        background-color: #0094ff;
    }


QSlider {
	height: 35px;
}


QSlider::groove:horizontal {
 background: #efd4a9;
border: 1px solid #dcc399;
height: 20px;
}

QSlider::sub-page:horizontal {
 background: #0094ff;
height: 15px;
}

QSlider::add-page:horizontal {
 background: #efd4a9;
border: 1px solid #dcc399;
height: 15px;
}

QSlider::handle:horizontal {
background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #fff, stop:1 #ddd);
border: 1px solid #444;
width: 13px;
margin-top: -5px;
margin-bottom: -5px;
}

QSlider::handle:horizontal:hover {
background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #fff, stop:1 #ddd);
border: 1px solid #444;
}



QRadioButton:hover {
    background: #535a57;
}

QRadioButton:pressed {
    background: #ff7500;
}

QRadioButton::indicator {
    width: 32px;
    height: 32px;
    padding: 5px 10px;
}

    QRadioButton::indicator::unchecked {
        image: url(forms/icons/check_empty.png);
    }

    QRadioButton::indicator:unchecked:hover {
        image: url(forms/icons/check_empty.png);
    }

    QRadioButton::indicator:unchecked:pressed {
        image: url(forms/icons/check_empty.png);
    }

    QRadioButton::indicator::checked {
        image: url(forms/icons/check_full.png);
    }

    QRadioButton::indicator:checked:hover {
        image: url(forms/icons/check_full.png);
    }

    QRadioButton::indicator:checked:pressed {
        image: url(forms/icons/check_full.png);
    }


QCheckBox {
    padding: 5px 10px;
}

    QCheckBox:hover {
        background: #535a57;
    }

    QCheckBox:pressed {
        background: #0094ff;
    }

